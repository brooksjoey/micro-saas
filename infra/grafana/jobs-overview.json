{
  "uid": "msaas-jobs-overview",
  "title": "Micro-SaaS Jobs & Queues Overview",
  "schemaVersion": 36,
  "version": 1,
  "tags": [
    "micro-saas",
    "jobs"
  ],
  "timezone": "",
  "refresh": "30s",
  "time": {
    "from": "now-6h",
    "to": "now"
  },
  "templating": {
    "list": [
      {
        "name": "datasource",
        "type": "datasource",
        "query": "prometheus",
        "label": "Datasource",
        "current": {
          "text": "Prometheus",
          "value": "Prometheus"
        },
        "hide": 0,
        "refresh": 1
      },
      {
        "name": "env",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "label": "Environment",
        "query": "label_values(msaas_job_processing_duration_seconds_count, env)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      },
      {
        "name": "service",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "label": "Service",
        "query": "label_values(msaas_job_processing_duration_seconds_count, service)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      },
      {
        "name": "queue_name",
        "type": "query",
        "datasource": {
          "type": "prometheus",
          "uid": "${datasource}"
        },
        "label": "Queue",
        "query": "label_values(msaas_queue_depth, queue_name)",
        "includeAll": true,
        "multi": true,
        "refresh": 1,
        "current": {
          "text": "All",
          "value": [
            "$__all"
          ]
        }
      }
    ]
  },
  "annotations": {
    "list": []
  },
  "panels": [
    {
      "id": 1,
      "type": "timeseries",
      "title": "Job Processing Latency p50 / p95 / p99",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "histogram_quantile(0.5, sum(rate(msaas_job_processing_duration_seconds_bucket{env=~\"$env\", service=~\"$service\"}[$__interval])) by (le, job_type))",
          "legendFormat": "p50 {{job_type}}"
        },
        {
          "refId": "B",
          "expr": "histogram_quantile(0.95, sum(rate(msaas_job_processing_duration_seconds_bucket{env=~\"$env\", service=~\"$service\"}[$__interval])) by (le, job_type))",
          "legendFormat": "p95 {{job_type}}"
        },
        {
          "refId": "C",
          "expr": "histogram_quantile(0.99, sum(rate(msaas_job_processing_duration_seconds_bucket{env=~\"$env\", service=~\"$service\"}[$__interval])) by (le, job_type))",
          "legendFormat": "p99 {{job_type}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "s",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Job Failure Rate",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "refId": "A",
          "expr": "sum(rate(msaas_job_errors_total{env=~\"$env\", service=~\"$service\"}[$__interval])) by (job_type) / sum(rate(msaas_job_processing_duration_seconds_count{env=~\"$env\", service=~\"$service\"}[$__interval])) by (job_type)",
          "legendFormat": "{{job_type}} failure rate"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Queue Depth by Queue",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "msaas_queue_depth{env=~\"$env\", service=~\"$service\", queue_name=~\"$queue_name\"}",
          "legendFormat": "{{queue_name}} ({{queue_kind}})"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Browser Worker Pending Messages",
      "datasource": {
        "type": "prometheus",
        "uid": "${datasource}"
      },
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 8
      },
      "targets": [
        {
          "refId": "A",
          "expr": "jobs_browser_pending_messages{env=~\"$env\"}",
          "legendFormat": "{{service}} {{queue_name}}"
        }
      ],
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "min": 0
        },
        "overrides": []
      },
      "options": {
        "legend": {
          "showLegend": true,
          "displayMode": "table",
          "placement": "bottom"
        },
        "tooltip": {
          "mode": "multi"
        }
      }
    }
  ]
}
